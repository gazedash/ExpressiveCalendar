{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "react/require-extension": "off",
    "newline-per-chained-call": 0,
    "generator-star-spacing": 0,
    "global-require": 0,
    "camelcase": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "arrow-body-style": 0,
    "max-len": [
      "error",
      200
    ],
    "new-cap": [0, {
      "newIsCap": true
    }]
  }
}
