{
  "name": "ExpressiveCalendar",
  "version": "0.0.1",
  "description": "Expressive calendar backend server",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --exec npm run babel-node -- src/app.js | ./node_modules/.bin/bunyan",
    "start": "npm run ./node_modules/.bin/babel-node -- src/app.js",
    "production": "npm run build && node ./lib/app.js",
    "babel-node": "./node_modules/.bin/babel-node",
    "eslint-fix": "./node_modules/.bin/eslint --ignore-pattern='*.spec.js' --fix src/",
    "fixtures": "./node_modules/.bin/babel-node ./src/fixtures/fixtures.js",
    "build": "npm run clean && ./node_modules/.bin/babel -d lib/ src/"
  },
  "author": "shoegazerwithak",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-stage-0": "6.16.0",
    "bcrypt": "0.8.7",
    "bluebird": "3.4.6",
    "body-parser": "1.15.2",
    "bunyan": "1.8.4",
    "cheerio": "0.22.0",
    "cors": "2.8.1",
    "express": "4.14.0",
    "hiredis": "0.5.0",
    "joi": "9.2.0",
    "jsonwebtoken": "7.1.9",
    "lodash": "4.16.4",
    "moment": "2.15.2",
    "mysql": "2.11.1",
    "redis": "2.6.2",
    "request": "2.76.0",
    "sequelize": "3.24.6",
    "socket.io": "1.5.1",
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0"
  },
  "devDependencies": {
    "babel-eslint": "7.1.0",
    "eslint": "3.9.0",
    "chai": "3.5.0",
    "mocha": "3.1.2",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1"
  }
}
