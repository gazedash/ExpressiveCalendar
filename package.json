{
  "name": "ExpressiveCalendar",
  "version": "0.0.1",
  "description": "Expressive calendar backend server",
  "main": "index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --exec npm run babel-node -- src/app.js | ./node_modules/.bin/bunyan",
    "start": "npm run ./node_modules/.bin/babel-node -- src/app.js",
    "production": "npm run build && node ./lib/app.js",
    "babel-node": "./node_modules/.bin/babel-node",
    "eslint-fix": "./node_modules/.bin/eslint --ignore-pattern='*.spec.js' --fix src/",
    "fixtures": "./node_modules/.bin/babel-node ./src/fixtures/fixtures.js",
    "build": "npm run clean && ./node_modules/.bin/babel -d lib/ src/"
  },
  "author": "shoegazerwithak",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-stage-0": "^6.5.0",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.1",
    "cheerio": "^0.22.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-async-wrap": "^1.0.0",
    "hiredis": "^0.5.0",
    "ioredis": "^2.3.0",
    "joi": "^9.0.4",
    "jsonwebtoken": "^7.1.9",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "mysql": "^2.11.1",
    "redis": "^2.6.2",
    "request": "^2.74.0",
    "sequelize": "^3.24.2",
    "socket.io": "^1.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "eslint": "^3.5.0",
    "chai": "3.5.0",
    "mocha": "3.0.2",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0"
  }
}
